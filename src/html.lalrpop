use std::str;

grammar;

pub Html: String = {
    Tag
}

pub Tag: String = {
    Text,
    "<" <t:ElementName> ">" Tag "</" <s:ElementName> ">" => {
        if t != s {
            return String::from("error")
        }
        t
    },
}

ElementName: String = {
    r"[a-z]+" => format!("{}", <>)
}

Text: String = {
    r"[a-z]+" => format!("{}", <>)
}
